mixin card(realName, displayName, profileImage, title)
    <div class="card mb-3" style="max-width: 540px;">
    <div class="row no-gutters">
        <div class="col-md-4">
        img.card-img(src= profileImage)
        </div>
        <div class="col-md-8">
        <div class="card-body">
            h4.card-title= displayName
                h6= realName
            p.card-text
                small.text-muted= title
            p.card-text

        </div>
        </div>
    </div>
    </div>
